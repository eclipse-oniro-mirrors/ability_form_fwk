              },
              {
                "header": {
                  "header_base": "//foundation/ability/form_fwk/frameworks/cj/form_provider/include",
                  "header_files": [
                  ]
                },
                "name": "//foundation/ability/form_fwk/frameworks/cj:cj_formProvider_ffi"
